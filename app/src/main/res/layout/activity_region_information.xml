<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context=".RegionInformation">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.cardview.widget.CardView
        android:layout_margin="15dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/trans"
        app:cardCornerRadius="20dp"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/empty_recyclerview_layout"

            >

            <TextView
                android:id="@+id/label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="50dp"
                android:fontFamily="cursive"
                android:text="Proprities"
                android:textColor="@color/blue"
                android:textSize="50dp"
                android:textStyle="italic" />

            <View
                android:layout_width="200dp"
                android:layout_height="2dp"
                android:layout_below="@id/label"
                android:layout_centerHorizontal="true"
                android:background="@color/blue" />



            <LinearLayout
                android:background="@drawable/empty_view"
                android:id="@+id/wifichamp"
                android:layout_width="match_parent"
                android:layout_height="50dp"

                android:layout_below="@id/label"
                android:layout_marginTop="50dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/logo_wifi"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_wifi" />

                <TextView
                    android:id="@+id/wifi_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:layout_marginTop="10dp"
                    />


            </LinearLayout>
            <LinearLayout
                android:id="@+id/reseauchamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/wifichamp"
                android:background="@drawable/empty_view"

                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/resrau_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_reseau" />

                <TextView
                    android:id="@+id/reseau_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"

                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/restochamp"
                android:background="@drawable/empty_view"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/reseauchamp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/resto_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_resto" />

                <TextView
                    android:id="@+id/resto_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"

                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/homechamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/restochamp"
                android:background="@drawable/empty_view"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/home_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_home" />

                <TextView
                    android:id="@+id/home_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"

                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/coffechamp"
                android:background="@drawable/empty_view"

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/homechamp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/coffe_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_coffee" />

                <TextView
                    android:id="@+id/coffe_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/barchamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/coffechamp"
                android:background="@drawable/empty_view"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/bar_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_bar" />

                <TextView
                    android:id="@+id/bar_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/kiosquechamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/barchamp"
                android:layout_marginTop="30dp"
                android:background="@drawable/empty_view"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/kiosque_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_kiosque" />

                <TextView
                    android:id="@+id/kiosque_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"

                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/gazchamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/kiosquechamp"
                android:background="@drawable/empty_view"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/gaz_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_gaz" />

                <TextView
                    android:id="@+id/gaz_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/storechamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/gazchamp"
                android:layout_marginTop="30dp"
                android:background="@drawable/empty_view"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/store_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_store" />

                <TextView
                    android:id="@+id/store_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/kidschamp"
                android:background="@drawable/empty_view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/storechamp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/kis_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_kids" />

                <TextView
                    android:id="@+id/kids_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/buschamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/kidschamp"
                android:background="@drawable/empty_view"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/bus_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_bus" />

                <TextView
                    android:id="@+id/bus_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/taxichamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/empty_view"
                android:layout_below="@id/metrochamp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/taxi_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_taxi" />

                <TextView
                    android:id="@+id/taxi_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/boatschamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/empty_view"
                android:layout_below="@id/taxichamp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/boats_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_boats" />

                <TextView
                    android:id="@+id/boats_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/cyclechamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/boatschamp"
                android:background="@drawable/empty_view"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/cycle_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_cycle" />

                <TextView
                    android:id="@+id/cycle_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rabbischamp"
                android:layout_width="match_parent"
                android:background="@drawable/empty_view"
                android:layout_height="wrap_content"
                android:layout_below="@id/cyclechamp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/rabbish_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_garbish" />

                <TextView
                    android:id="@+id/rabbish_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/metrochamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/empty_view"
                android:layout_below="@id/buschamp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/metro_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_metro" />

                <TextView
                    android:id="@+id/metro_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/parkchamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rabbischamp"
                android:background="@drawable/empty_view"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/park_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_park" />

                <TextView
                    android:id="@+id/park_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/energiechamp"
                android:layout_width="match_parent"
                android:background="@drawable/empty_view"
                android:layout_height="wrap_content"
                android:layout_below="@id/parkchamp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/energie_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_energie" />

                <TextView
                    android:id="@+id/energie_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_marginBottom="10dp"
                android:id="@+id/lightchamp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/empty_view"
                android:layout_below="@id/energiechamp"
                android:layout_marginTop="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/light_logo"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="20dp"

                    android:src="@drawable/ic_light" />

                <TextView
                    android:id="@+id/light_region"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:text="wifi"
                    android:textColor="@color/black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    />
            </LinearLayout>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
</RelativeLayout>
</ScrollView>